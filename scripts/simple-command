_SC_GENII_INSTALL_DIR=%{INSTALL_PATH}
_SC_LOCAL_JAVA_DIR=$_SC_GENII_INSTALL_DIR/Java/linux-i586/jre

exec "$_SC_LOCAL_JAVA_DIR/bin/java" -Xms32M -Xmx128M -classpath "%{INSTALL_PATH}/ext/bouncycastle/bcprov-jdk15-133.jar:%{INSTALL_PATH}/lib/GenesisII-security.jar:%{INSTALL_PATH}/lib/morgan-utilities.jar:%{INSTALL_PATH}/lib:%{INSTALL_PATH}/security:%{INSTALL_PATH}/lib/GenesisII-client.jar" -Dlog4j.configuration=genesisII.log4j.properties "-Djava.library.path=%{INSTALL_PATH}/jni-lib" "-Dedu.virginia.vcgr.genii.install-base-dir=%{INSTALL_PATH}" org.morgan.util.launcher.Launcher "%{INSTALL_PATH}/jar-desc.xml" $*
